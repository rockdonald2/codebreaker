{
  "name": "codebreaker",
  "version": "1.0.0",
  "description": "Fallout style codebreaker game",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Codebreaker\"",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/icon.png --prune=true --out=release-builds"
  },
  "author": "Lukacs Zsolt",
  "license": "MIT",
  "devDependencies": {
    "electron": "^6.0.8",
    "electron-packager": "^14.0.6"
  },
  "dependencies": {
    "electron": "^6.0.8",
    "jquery": "^3.5.0"
  }
}
